{
    "name": "dpfx/laravel-livewire-wizards",
    "description": "A livewire wizard for laravel.",
    "keywords": [
        "laravel",
        "livewire",
        "wizards",
        "multiple-steps",
        "forms"
    ],
    "homepage": "https://github.com/dpfx/laravel-livewire-wizards",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dpfx\\LaravelLivewireWizards\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpfx\\LaravelLivewireWizards\\ServiceProvider"
            ]
        }
    }
}
